#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eyalcohen/dev/novel/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/Users/eyalcohen/dev/novel/node_modules/.pnpm/prettier@2.8.8/node_modules:/Users/eyalcohen/dev/novel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eyalcohen/dev/novel/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/Users/eyalcohen/dev/novel/node_modules/.pnpm/prettier@2.8.8/node_modules:/Users/eyalcohen/dev/novel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/bin-prettier.js" "$@"
fi
